<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

    <changeSet id="ARTICLE_TABLE_1" author="stas.shilovich@gmail.com">
        <createTable tableName="T_ARTICLE">
            <column autoIncrement="true" name="F_ID" type="BIGINT">
                <constraints primaryKey="true"/>
            </column>
            <column name="F_DATE" type="DATE"/>
            <column name="F_TITLE" type="VARCHAR(100)"/>
            <column name="F_CONTENT" type="VARCHAR(1000)"/>
            <column name="F_AUTHOR_INFO" type="VARCHAR(100)"/>
            <column name="F_DELETED" type="BOOLEAN"/>
        </createTable>
    </changeSet>

</databaseChangeLog>