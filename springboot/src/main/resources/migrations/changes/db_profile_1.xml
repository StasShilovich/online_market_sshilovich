<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

    <changeSet id="PROFILE_TABLE_1" author="stas.shilovich@gmail.com">
        <createTable tableName="T_PROFILE">
            <column autoIncrement="true" name="F_ID" type="BIGINT">
                <constraints nullable="false" foreignKeyName="FK_USER_PROFILE" references="T_USER(F_ID)"
                             primaryKey="true"/>
            </column>
            <column name="F_ADDRESS" type="VARCHAR(50)"/>
            <column name="F_PHONE_NUMBER" type="VARCHAR(500)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>